<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.model">

    <class name="Customer" table="customer">
        <id name="customerId" column="customerId">
            
        </id>
        <property name="customerName" type="string" column="customerName"/>
        <property name="country" type="string" column="country"/>
        <property name="state" type="string" column="state"/>
        <property name="city" type="string" column="city"/>
         <property name="address" type="string" column="address"/>
        <property name="emailId" type="string" column="emailId"/>
       	<property name="contactNumber" type="string" column="contactNumber"/>
    </class>

</hibernate-mapping>