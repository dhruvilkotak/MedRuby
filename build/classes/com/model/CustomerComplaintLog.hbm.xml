<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.model">

    <class name="CustomerComplaintLog" table="customer_complaint_log">
        <id name="complaintNumber" column="complaintNumber">
           
        </id>
        <property name="complaintDate" type="string" column="complaintDate"/>
       
       	<property name="receivedBy" type="string" column="receivedBy"/>
       	
       	<property name="salesOrderId" type="string" column="salesOrderId"/>
    </class>

</hibernate-mapping>