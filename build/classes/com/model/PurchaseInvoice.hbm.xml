<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.model">

    <class name="PurchaseInvoice" table="purchase_invoice">
        <id name="invoiceNumber" column="invoiceNumber">
            
        </id>
        <property name="poNumber" type="string" column="poNumber"/>
        <property name="fileName" type="string" column="fileName"/>
        <property name="invoiceDate" type="string" column="invoiceDate"/>
       	
    </class>

</hibernate-mapping>