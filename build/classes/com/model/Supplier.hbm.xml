<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.model">

    <class name="Supplier" table="supplier">
        <id name="supplierId" column="supplierId">
           
        </id>
        <property name="supplierName" type="string" column="supplierName"/>
        <property name="address" type="string" column="address"/>
       	<property name="emailId" type="string" column="emailId"/>
       	<property name="phoneNumber" type="string" column="phoneNumber"/>
       	
    </class>

</hibernate-mapping>